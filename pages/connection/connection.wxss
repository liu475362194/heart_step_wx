page{
  background-color: #012847;
  height: 98%;
  position: absolute;
}
.scroll{
  height: 100%;
}
.chart {
  height: 170px;
  position: relative;
  top: 280px;
  /*background-color: #f0f;*/
}

.chart_canvas {
  height: 200px;
}
.head{
  width: 100%;
}
.connect_image{
  position: absolute;
  top: 15px;
  left: 15px;
  width: 25px;
  height: 25px;
}
.heart{
  position: absolute;
  top: 50px;
  /*background-color: #fff;*/
  width: 100%;
  height: 200px;
  text-align: center;
}

.heart_image{
  width: 55%;
  height: 200px;
}
.heart_content{
  color: #fff;
  display: block;
  position: absolute;
  top: 80px;
  left: 50%;
}
.heart_text{
  font-size: 25px;
  font-weight: bold;
  position: relative;
  top: 5px;
  right: 40px;
}
.da_heart{
  width: 25px;
  height: 25px;
  position: absolute;
  left: 13px;
  top: -5px;
}
.xiao_heart{
  width: 20px;
  height: 20px;
  position: absolute;
  left: 15px;
  top: -2px;
}
.heart_cimeifen{
  font-size: 12px;
  width: 50px;
  position: absolute;
  top: 25px;
  left: 0px;
}

.sport{
  width: 100%;
